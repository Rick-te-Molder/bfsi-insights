{
  "name": "bfsi-agent-api",
  "version": "1.0.0",
  "description": "Agent API Service for BFSI Insights",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "validate:prompts": "node src/scripts/validate-agent-registry.js",
    "lint": "eslint \"src/**/*.js\"",
    "typecheck": "tsc -p tsconfig.checkjs.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@supabase/supabase-js": "^2.90.1",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "fast-xml-parser": "^5.3.2",
    "langsmith": "^0.2.15",
    "openai": "^6.14.0",
    "playwright": "^1.57.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
